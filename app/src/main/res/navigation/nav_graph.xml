<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/privacy_fragment">

    <fragment
        android:id="@+id/privacy_fragment"
        android:name="com.example.settingtest.Fragment.PrivacyFragment"
        tools:layout="@layout/fragment_privacy">
        <action
            android:id="@+id/action_privacy_fragment_to_privacy_statement_fragment"
            app:destination="@id/privacy_statement_fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"/>
        <action
            android:id="@+id/action_privacy_fragment_to_app_manage_fragment"
            app:destination="@id/app_manage_fragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"/>
    </fragment>
    <fragment
        android:id="@+id/privacy_statement_fragment"
        android:name="com.example.settingtest.Fragment.PrivacyStatementFragment"
        tools:layout="@layout/fragment_privacy_statement">
        <action
            android:id="@+id/action_privacy_statement_fragment_to_privacy_fragment"
            app:destination="@id/privacy_fragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/app_manage_fragment"
        android:name="com.example.settingtest.Fragment.AppManageFragment"
        tools:layout="@layout/fragment_app_manage">
        <action
            android:id="@+id/action_app_manage_fragment_to_privacy_fragment"
            app:destination="@id/privacy_fragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"/>
    </fragment>
</navigation>